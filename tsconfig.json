{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "Node",
    "lib": ["dom", "es2015"],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "types": [
      "@nuxt/types",
      "@nuxtjs/axios",
      "@nuxt/content",
      "@types/node",
      "@nuxt/config"
    ]
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
